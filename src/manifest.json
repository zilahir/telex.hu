{
	"name": "www.telex.hu",
	"options_page": "options.html",
	"version": "0.0.1",
	"background": {
		"scripts": ["background.bundle.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon-34.png"
	},
	"permissions": [
		"https://www.telex.hu/",
		"https://www.telex.hu/*",
		"tabs",
		"webNavigation",
		"history",
		"storage",
		"notifications"
	],
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"icons": {
		"128": "icon-128.png"
	},
	"content_scripts": [{
		"matches": ["https://www.telex.hu/*"],
		"js": ["contentScript.bundle.js", "contentScriptSubPage.bundle.js"],
		"css": ["content.styles.css"]
	}],
	"web_accessible_resources": [
		"content.styles.css",
		"icon-128.png",
		"icon-34.png"
	],
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}